#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/react project/project1/node_modules/.pnpm/typescript@4.5.2/node_modules/typescript/bin/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/typescript@4.5.2/node_modules/typescript/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/typescript@4.5.2/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/node_modules:/mnt/d/react project/project1/node_modules:/mnt/d/react project/node_modules:/mnt/d/node_modules:/mnt/d/react project/project1/node_modules/typescript/bin/node_modules:/mnt/d/react project/project1/node_modules/typescript/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/react project/project1/node_modules/.pnpm/typescript@4.5.2/node_modules/typescript/bin/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/typescript@4.5.2/node_modules/typescript/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/typescript@4.5.2/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/node_modules:/mnt/d/react project/project1/node_modules:/mnt/d/react project/node_modules:/mnt/d/node_modules:/mnt/d/react project/project1/node_modules/typescript/bin/node_modules:/mnt/d/react project/project1/node_modules/typescript/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typescript/bin/tsc" "$@"
else
  exec node  "$basedir/../typescript/bin/tsc" "$@"
fi
