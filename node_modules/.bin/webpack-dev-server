#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/react project/project1/node_modules/.pnpm/webpack-dev-server@4.6.0_webpack-cli@4.9.1+webpack@5.65.0/node_modules/webpack-dev-server/bin/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/webpack-dev-server@4.6.0_webpack-cli@4.9.1+webpack@5.65.0/node_modules/webpack-dev-server/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/webpack-dev-server@4.6.0_webpack-cli@4.9.1+webpack@5.65.0/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/node_modules:/mnt/d/react project/project1/node_modules:/mnt/d/react project/node_modules:/mnt/d/node_modules:/mnt/d/react project/project1/node_modules/webpack-dev-server/bin/node_modules:/mnt/d/react project/project1/node_modules/webpack-dev-server/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/react project/project1/node_modules/.pnpm/webpack-dev-server@4.6.0_webpack-cli@4.9.1+webpack@5.65.0/node_modules/webpack-dev-server/bin/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/webpack-dev-server@4.6.0_webpack-cli@4.9.1+webpack@5.65.0/node_modules/webpack-dev-server/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/webpack-dev-server@4.6.0_webpack-cli@4.9.1+webpack@5.65.0/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/node_modules:/mnt/d/react project/project1/node_modules:/mnt/d/react project/node_modules:/mnt/d/node_modules:/mnt/d/react project/project1/node_modules/webpack-dev-server/bin/node_modules:/mnt/d/react project/project1/node_modules/webpack-dev-server/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../webpack-dev-server/bin/webpack-dev-server.js" "$@"
else
  exec node  "$basedir/../webpack-dev-server/bin/webpack-dev-server.js" "$@"
fi
