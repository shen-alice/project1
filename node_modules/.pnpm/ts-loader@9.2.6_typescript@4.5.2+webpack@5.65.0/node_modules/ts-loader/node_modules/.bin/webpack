#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/react project/project1/node_modules/.pnpm/webpack@5.65.0_webpack-cli@4.9.1/node_modules/webpack/bin/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/webpack@5.65.0_webpack-cli@4.9.1/node_modules/webpack/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/webpack@5.65.0_webpack-cli@4.9.1/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/node_modules:/mnt/d/react project/project1/node_modules:/mnt/d/react project/node_modules:/mnt/d/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/ts-loader@9.2.6_typescript@4.5.2+webpack@5.65.0/node_modules/ts-loader/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/ts-loader@9.2.6_typescript@4.5.2+webpack@5.65.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/react project/project1/node_modules/.pnpm/webpack@5.65.0_webpack-cli@4.9.1/node_modules/webpack/bin/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/webpack@5.65.0_webpack-cli@4.9.1/node_modules/webpack/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/webpack@5.65.0_webpack-cli@4.9.1/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/node_modules:/mnt/d/react project/project1/node_modules:/mnt/d/react project/node_modules:/mnt/d/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/ts-loader@9.2.6_typescript@4.5.2+webpack@5.65.0/node_modules/ts-loader/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/ts-loader@9.2.6_typescript@4.5.2+webpack@5.65.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../webpack@5.65.0_webpack-cli@4.9.1/node_modules/webpack/bin/webpack.js" "$@"
else
  exec node  "$basedir/../../../../../webpack@5.65.0_webpack-cli@4.9.1/node_modules/webpack/bin/webpack.js" "$@"
fi
