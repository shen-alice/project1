#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/react project/project1/node_modules/.pnpm/is-docker@2.2.1/node_modules/is-docker/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/is-docker@2.2.1/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/node_modules:/mnt/d/react project/project1/node_modules:/mnt/d/react project/node_modules:/mnt/d/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/open@8.4.0/node_modules/open/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/open@8.4.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/react project/project1/node_modules/.pnpm/is-docker@2.2.1/node_modules/is-docker/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/is-docker@2.2.1/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/node_modules:/mnt/d/react project/project1/node_modules:/mnt/d/react project/node_modules:/mnt/d/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/open@8.4.0/node_modules/open/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/open@8.4.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../is-docker@2.2.1/node_modules/is-docker/cli.js" "$@"
else
  exec node  "$basedir/../../../../../is-docker@2.2.1/node_modules/is-docker/cli.js" "$@"
fi
