#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/react project/project1/node_modules/.pnpm/import-local@3.0.3/node_modules/import-local/fixtures/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/import-local@3.0.3/node_modules/import-local/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/import-local@3.0.3/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/node_modules:/mnt/d/react project/project1/node_modules:/mnt/d/react project/node_modules:/mnt/d/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/react project/project1/node_modules/.pnpm/import-local@3.0.3/node_modules/import-local/fixtures/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/import-local@3.0.3/node_modules/import-local/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/import-local@3.0.3/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/node_modules:/mnt/d/react project/project1/node_modules:/mnt/d/react project/node_modules:/mnt/d/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../fixtures/cli.js" "$@"
else
  exec node  "$basedir/../../fixtures/cli.js" "$@"
fi
