#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/react project/project1/node_modules/.pnpm/acorn@8.6.0/node_modules/acorn/bin/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/acorn@8.6.0/node_modules/acorn/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/acorn@8.6.0/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/node_modules:/mnt/d/react project/project1/node_modules:/mnt/d/react project/node_modules:/mnt/d/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/webpack@5.65.0_webpack-cli@4.9.1/node_modules/webpack/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/webpack@5.65.0_webpack-cli@4.9.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/react project/project1/node_modules/.pnpm/acorn@8.6.0/node_modules/acorn/bin/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/acorn@8.6.0/node_modules/acorn/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/acorn@8.6.0/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/node_modules:/mnt/d/react project/project1/node_modules:/mnt/d/react project/node_modules:/mnt/d/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/webpack@5.65.0_webpack-cli@4.9.1/node_modules/webpack/node_modules:/mnt/d/react project/project1/node_modules/.pnpm/webpack@5.65.0_webpack-cli@4.9.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../acorn@8.6.0/node_modules/acorn/bin/acorn" "$@"
else
  exec node  "$basedir/../../../../../acorn@8.6.0/node_modules/acorn/bin/acorn" "$@"
fi
